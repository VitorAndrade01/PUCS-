#include <stdio.h>

int main() {
    int termo1 = 0, termo2 = 1, proximo, i;

    printf("Série de Fibonacci até o 20º termo:\n");

    for (i = 1; i <= 20; i++) {
        printf("%d ", termo1);
        proximo = termo1 + termo2;
        termo1 = termo2;
        termo2 = proximo;
    }

    printf("\n");
    return 0;
}
