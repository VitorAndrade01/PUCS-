1. Quest√£o IFSC
A) (20-15)/2 = 2
------------------------------------------------------
R: #include <stdio.h>
int main() {
 int a = 20, b = 15, c = 2;
 int div = (a - b)/ c;

 printf ("resultado: %d" , div);
 return 0;
}
------------------------------------------------------
B) 20-15/2 = 13
------------------------------------------------------
R: #include <stdio.h>
int main() {
 int x = 20, y = 15, c = 2;
 printf("%d", x - y/2);
 return 0;
}
C) 2*5/20 + 30/15*2 = 4
R: #include <stdio.h>
int main() {
 int a = 2, b = 5, c = 20, d = 30, e = 15;
 int div = (a * b / c + d / e * a);

 printf ("resultado: %d" , div);
 return 0;
}
--------------------------------------------------------------
D) 2*(5/20) + 30/(15*2) = 1
R: #include <stdio.h>
int main() {
 int a = 2, b = 5, c = 20, d = 30, e = 15;
 int div = a * (b / c) + d / (e * a);

 printf ("resultado: %d" , div);
 return 0;
}
---------------------------------------------------------------
E) 23 div 4 = 5
R: #include <stdio.h>
int main() {
 int a = 23, b = 4;
 int div = a/b;

 printf ("resultado: %d" , div);
 return 0;
}
----------------------------------------------------------------
F) 23 mod 4 = 3
R: #include <stdio.h>
int main() {
 int a = 23, b = 4;
 int div = (a % b);

 printf ("resultado: %d" , div);
 return 0;
}
-------------------------------------------------------------
G) 35 div 6 + 2 = 7
R: #include <stdio.h>
int main() {
 int a = 35, b = 6, c = 2;
 int div = (a / b + c);

 printf ("resultado: %d" , div);
 return 0;
}
--------------------------------------------------------------------]
H) 35 div 6 - 2 = 3
R: #include <stdio.h>
int main() {
 int a = 35, b = 6, c = 2;
 int div = (a / b - c);

 printf ("resultado: %d" , div);
 return 0;
}
