i) 35 div 6 * 2 = 10
R: #include <stdio.h>
int main() {
 int a = 35, b = 6, c = 2;
 int div = (a / b * c);

 printf ("resultado: %d" , div);
 return 0;
}
--------------------------------------------------------------------------
j) sqrt(625) = 25
R: #include <stdio.h>
#include <math.h>
int main() {
 int a = 625.0;
 int raiz = sqrt (a);

 printf ("resultado: %d" , raiz);
 return 0;
}
--------------------------------------------------------------------------
k) sqr(20) = 4
R: #include <stdio.h>
#include <math.h>
int main() {
 int a = 20;
 int raiz = sqrt (a);

 printf ("resultado: %d" , raiz);
 return 0;
}
------------------------------------------------------------------------------
l) 2 + sqrt(21 div 5) = 4
R: #include <stdio.h>
#include <math.h>
int main() {
 int a = 2, b = 21, c = 5;
 int div = a + sqrt(b / c);

 printf ("resultado: %d" , div);
 return 0;
}
