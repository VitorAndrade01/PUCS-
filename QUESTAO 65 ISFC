#include <stdio.h>

int main() {
    float nota1, nota2, media;

    // Leitura das notas
    printf("Digite a primeira nota: ");
    scanf("%f", &nota1);
    printf("Digite a segunda nota: ");
    scanf("%f", &nota2);

    // Cálculo da média
    media = (nota1 + nota2) / 2;

    // Verificação da aprovação
    if (media > 5.0) {
        printf("Aprovado! Sua média foi: %.2f\n", media);
    } else {
        printf("Reprovado! Sua média foi: %.2f\n", media);
    }

    return 0;
}
